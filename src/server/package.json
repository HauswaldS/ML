{
  "name": "ml_server",
  "version": "1.0.0",
  "description": "Thermacote website, server side made with Express",
  "scripts": {
    "watch:dev": "cross-env NODE_ENV=development && nodemon ./server.js --exec babel-node --watch",
    "build:dev": "cross-env NODE_ENV=development && rimraf dist && babel . -d dist --ignore node_modules",
    "build:prod": "cross-env NODE_ENV=production && rimraf dist && babel . -d dist --ignore node_modules",
    "start:dev": "cross-env NODE_ENV=development && babel-node ./server.js",
    "start:prod": "cross-env NODE_ENV=production && babel-node ./server.js"
  },
  "author": "Hauswald",
  "license": "ISC",
  "dependencies": {
    "auth0": "^2.14.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "2.0.0",
    "dotenv-expand": "^0.1.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "fs": "0.0.1-security",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "mongodb": "^3.1.8",
    "mongoose": "^5.3.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.2.0",
    "eslint": "^4.17.0",
    "nodemon": "^1.17.5",
    "rimraf": "^2.6.2"
  }
}
